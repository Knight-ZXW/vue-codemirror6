{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vue-codemirror6",
  "version": "0.1.6",
  "description": "CodeMirror6 Component for vue2",
  "keywords": [
    "vuejs",
    "vue",
    "vue-components",
    "vue-codemirror",
    "code-editor",
    "text-editor",
    "vue2",
    "web-editor",
    "vue-plugin",
    "vue-component",
    "codemirror-editor",
    "vue-resource",
    "codemirror6"
  ],
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/vue-codemirror6",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/vue-codemirror6.git"
  },
  "bugs": {
    "url": "https://github.com/logue/vue-codemirror6/issues"
  },
  "engines": {
    "node": ">=16.15.0",
    "yarn": ">=1.22.4"
  },
  "license": "MIT",
  "main": "./dist/vue-codemirror6.umd.js",
  "module": "./dist/vue-codemirror6.es.js",
  "browser": {
    "./sfc": "./src/components/CodeMirror.vue"
  },
  "exports": {
    ".": {
      "import": "./dist/vue-codemirror6.es.js",
      "require": "./dist/vue-codemirror6.umd.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "/dist",
    "/src/components/CodeMirror.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "clean": "rimraf node_modules/.vite",
    "build": "vite build & vue-tsc --declaration --emitDeclarationOnly",
    "build:clean": "rimraf dist",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "lint:markup": "vue-tsc --noEmit",
    "switch:2": "vue-demi-switch 2 & npx vue-demi-fix",
    "switch:3": "vue-demi-switch 3 vue3 & npx vue-demi-fix",
    "prepare": "husky install",
    "postinstall": "npx vue-demi-fix"
  },
  "dependencies": {
    "vue-demi": "latest"
  },
  "peerDependencies": {
    "@codemirror/basic-setup": "latest",
    "@codemirror/state": "latest",
    "@codemirror/view": "latest",
    "@vue/composition-api": "latest",
    "vue": "^2.6.14 || ^3.2.33"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@codemirror/basic-setup": "^0.20.0",
    "@codemirror/lang-markdown": "^0.20.0",
    "@codemirror/language": "^0.20.1",
    "@codemirror/lint": "^0.20.2",
    "@modyqyw/vite-plugin-eslint": "^1.3.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.34",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@vue/composition-api": "^1.6.1",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.15.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-prettier-vue": "^3.1.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "eslint-plugin-vue": "^9.0.1",
    "eslint-plugin-vuejs-accessibility": "^1.1.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "lodash": "^4.17.21",
    "prettier": "^2.6.2",
    "prettier-plugin-jsdoc": "^0.3.38",
    "prettier-plugin-md-nocjsp": "^1.2.0",
    "prettier-plugin-sort-imports": "^1.6.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-plugin-vue2": "^2.0.1",
    "vue": "^2.6.14",
    "vue-eslint-parser": "^9.0.2",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.34.15"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,htm,html}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*": "prettier -w -u"
  }
}
